str1 = ''
str2 = 'GGGGGTAGACCAAGA'
str3 = 'GGGGGATACAAGA'
commonMotifs = []
for j in range(len(str1)):
    for i in range(j, len(str1)):
        commonMotifs.append(str1[j:i+1])
        lenOfMot = i - j
     



print(commonMotifs)
maxLen = 0
maxItem = ''

for i in commonMotifs:
    if len(i)>maxLen:
        maxItem = i
        maxLen = len(i)


print(maxItem)
        
